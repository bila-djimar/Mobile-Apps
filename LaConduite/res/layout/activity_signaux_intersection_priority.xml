<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.laconduite.ActivitySignauxIntersectionPriority"
    tools:ignore="MergeRootFrame" >

<TableRow
        android:id="@+id/tableRowIntersectionpriorite1"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:gravity="center"
        android:background="#FFFFFF" >

       <TextView
            android:id="@+id/textViewIntersectionpriorite1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="        " 
            android:textSize="10sp" />
      

        
    </TableRow>   
     
    
    
<TableRow
        android:id="@+id/tableRowIntersectionpriorite"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:gravity="center"
        android:background="#FFFFFF" >

        <ImageView
            android:id="@+id/imageViewIntersectionpriorite"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:src="@drawable/image_entete" />

    </TableRow>
    
   <TableRow
        android:id="@+id/tableRowIntersectionpriorite2"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:gravity="center"
        android:background="#FFFFFF" >

       <TextView
            android:id="@+id/textViewIntersectionpriorite2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="        " 
            android:textSize="10sp" />
      

        
    </TableRow>  
    
    <WebView
        android:id="@+id/webViewIntersectionPriorite"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</TableLayout>