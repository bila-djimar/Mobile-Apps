<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.laconduite.ActivitySignauxLumineux"
    tools:ignore="MergeRootFrame" >

<TableRow
        android:id="@+id/tableRowArretLumineux1"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:gravity="center"
        android:background="#FFFFFF" >

       <TextView
            android:id="@+id/textViewArretLumineux1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="        " 
            android:textSize="10sp" />
      

        
    </TableRow>   
    



<TableRow
        android:id="@+id/tableRowLumineux"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:gravity="center"
        android:background="#FFFFFF" >

        <ImageView
            android:id="@+id/imageViewLumineux"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:src="@drawable/image_entete" />

    </TableRow>  
    
<TableRow
        android:id="@+id/tableRowArretLumineux2"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:gravity="center"
        android:background="#FFFFFF" >

       <TextView
            android:id="@+id/textViewArretLumineux2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="        " 
            android:textSize="10sp" />
      

        
    </TableRow>       
    
    
    
    <WebView
        android:id="@+id/webViewLumineux"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</TableLayout>
    