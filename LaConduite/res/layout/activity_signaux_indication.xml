<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.laconduite.ActivitySignauxIndication"
    tools:ignore="MergeRootFrame" >

 <TableRow
        android:id="@+id/tableRowSignauxIndication1"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:gravity="center"
        android:background="#FFFFFF" >

       <TextView
            android:id="@+id/textViewSignauxIndication1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="        " 
            android:textSize="10sp" />
      

        
    </TableRow>   
      
    
    
    
<TableRow
        android:id="@+id/tableRowSignauxIndication"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:gravity="center"
        android:background="#FFFFFF" >

        <ImageView
            android:id="@+id/imageViewSignauxIndication"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:src="@drawable/image_entete" />

    </TableRow> 
    
    <TableRow
        android:id="@+id/tableRowSignauxIndication2"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:gravity="center"
        android:background="#FFFFFF" >

       <TextView
            android:id="@+id/textViewSignauxIndication2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="        " 
            android:textSize="10sp" />
      

        
    </TableRow>   
      
    
    
    <WebView
        android:id="@+id/webViewSignauxIndication"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</TableLayout>

    